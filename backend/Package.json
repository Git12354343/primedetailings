{
  "name": "prime-detailing-backend",
  "version": "1.0.0",
  "description": "Prime Detailing API Server with Authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.1",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "car-detailing",
    "booking-system",
    "sms-verification",
    "jwt-authentication",
    "express",
    "prisma",
    "mysql"
  ],
  "author": "Prime Detailing Montreal",
  "license": "MIT"
}
